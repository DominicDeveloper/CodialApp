<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".AboutFragment">
    <include
        android:id="@+id/includeed"
        layout="@layout/custombar_only_for_back"/>

        <TextView
            app:layout_constraintTop_toBottomOf="@id/includeed"
            app:layout_constraintRight_toRightOf="@id/includeed"
            app:layout_constraintLeft_toLeftOf="@id/includeed"
            android:id="@+id/text_about"
            android:textSize="20sp"
            android:padding="8dp"
            android:layout_margin="10dp"
            android:textColor="#2A1065"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/app_name" />


</androidx.constraintlayout.widget.ConstraintLayout>